'classnames'\n\nimport * as recommendApis from 'apis/recommendation'\nimport useAsyncRequest from 'hooks/useAsyncRequest'\nimport { TARGET_TYPE } from 'constants/constants'\n\nimport BannerItem from './banner-item'\nimport styles from './index.module.scss'\n\nconst { useState, useEffect, useMemo } = React\n\nconst Banner = () => {\n  const [currentMid, setCurrentMid] = useState(0)\n\n  const [state, getBannerList] = useAsyncRequest(recommendApis.getBannerList)\n  const { value: banners = [], loading: getBannerLoading } = state\n  console.log(getBannerLoading)\n\n  useEffect(() => {\n    const postData = { type: 1 }\n    getBannerList(postData)\n  }, [])\n\n  const bannersClassName = useMemo(\n    () => {\n      const len = banners.length\n      const left = (currentMid - 1 + len) % len\n      const right = (currentMid + 1) % len\n      return {\n        [currentMid]: styles.middle,\n        [left]: styles.left,\n        [right]: styles.right\n      }\n    },\n    [currentMid, banners]\n  )\n\n  const handleBannerChange = (index: number) => {\n    setCurrentMid(index)\n  }\n\n  const handleBannerItemClick = async (\n    index: number,\n    targetId: number,\n    targetType: number\n  ) => {\n    setCurrentMid(index)\n    if (targetType === TARGET_TYPE.MUSIC) {\n      console.log(targetId)\n    }\n  }\n\n  return (\n    <div className={styles['banner-wrap']}>\n      <div className={styles.banners}>\n        {banners.map(({ pic, typeTitle, targetId, targetType }, index) => {\n          const className = bannersClassName[index] || styles.hidden\n\n          return (\n            <BannerItem\n              key={pic}\n              title={typeTitle}\n              pic={pic}\n              className={className}\n              onClick={() => handleBannerItemClick(index, targetId, targetType)}\n            />\n          )\n        })}\n      </div>\n      <div className={styles.dots}>\n        {banners.map((dot, index) => {\n          return (\n            <div\n              key={dot.pic}\n              className={cn(\n                styles.dot,\n                index === currentMid ? styles['dot-active'] : ''\n              )}\n              onMouseOver={() => handleBannerChange(index)}\n            />\n          )\n        })}\n      </div>\n    </div>\n  )\n}\n\nexport default Banner\n",["120","121"],"/Users/yejianfei/my_project/fate-music/src/components/link-title/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/recommend-song-item/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/utils/index.ts",[],"/Users/yejianfei/my_project/fate-music/src/components/play-count/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/App.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/my-fm/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/discovery/song-list/index.tsx",["122"],"import React from 'react'\n\nimport * as recommendApis from 'apis/recommendation'\nimport useAsyncRequest from 'hooks/useAsyncRequest'\nimport RecommendSongItem from 'components/recommend-song-item'\n\nimport styles from './index.module.scss'\n\nconst { useEffect } = React\n/**\n * 发现 - 歌单\n */\nconst SongList = () => {\n  const [oneHighqualityState, getOneHighquality] = useAsyncRequest(\n    recommendApis.getOneHighquality\n  )\n  const { value: highquality = {} } = oneHighqualityState\n\n  const [state, getRecommendResource] = useAsyncRequest(\n    recommendApis.getRecommendResource\n  )\n  const { value: recommendSongs = [] } = state\n\n  useEffect(() => {\n    getOneHighquality()\n    getRecommendResource()\n  }, [])\n\n  return (\n    <div className={styles['song-list-wrap']}>\n      <div className={styles['highquality-song-wrap']}>\n        <div className={styles['highquality-song-row']}>\n          {highquality.coverImgUrl && (\n            <img src={highquality.coverImgUrl} loading=\"lazy\" alt=\"\" />\n          )}\n          <div className={styles['highquality-song-info']}>\n            <div className={styles['highquality-tag']}>精品歌单</div>\n            <div className={styles['highquality-name']}>{highquality.name}</div>\n            <div className={styles['highquality-tip']}>\n              {highquality.copywriter}\n            </div>\n          </div>\n        </div>\n      </div>\n      <div className={styles['song-cont']}>\n        {recommendSongs.map(item => {\n          return <RecommendSongItem key={item.id} songItem={item} />\n        })}\n      </div>\n    </div>\n  )\n}\n\nexport default SongList\n","/Users/yejianfei/my_project/fate-music/src/views/download/files/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/my-collect/album/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/music-player/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/header/navbar/nav.constants.js",[],"/Users/yejianfei/my_project/fate-music/src/reportWebVitals.js",[],["123","124"],"/Users/yejianfei/my_project/fate-music/src/views/discovery/latest-songs/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/header/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/sidebar/user/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/sidebar/menu/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/hooks/useAsyncRequest.ts",["125","126"],"/Users/yejianfei/my_project/fate-music/src/views/videos/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/discovery/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/itunes/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/my-collect/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/fm/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/discovery/recommendation/banner/banner-item/index.tsx",[],{"ruleId":"127","severity":1,"message":"128","line":26,"column":6,"nodeType":"129","endL