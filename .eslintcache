ct, useState } = React\n\nconst User = () => {\n  const [state, dispatch] = useContext(ReducerContext)\n  const { showLoginModal, token } = state.login\n\n  const [showLoginVisible, setShowLogin] = useState(false)\n  // 用户是否已登录\n  const [userLogged, setUserLogged] = useState(false)\n  const userInfo: any = getUserInfo()\n\n  const handleShowLogin = () => {\n    if (userLogged) {\n      dispatch({ type: 'SHOW_USER_INFO' })\n    } else {\n      if (showLoginVisible) {\n        dispatch({ type: 'HIDE_LOGIN' })\n      } else {\n        dispatch({ type: 'SHOW_LOGIN' })\n      }\n    }\n  }\n\n  useEffect(\n    () => {\n      setShowLogin(showLoginModal)\n      if ((userInfo && userInfo.token) || token) {\n        setUserLogged(true)\n      } else {\n        setUserLogged(false)\n      }\n    },\n    [showLoginModal, token]\n  )\n\n  return (\n    <div className={styles['user-wrap']}>\n      <div className={styles['user-img']}>\n        <img\n          src={userLogged ? userInfo.profile.avatarUrl : defaultUser}\n          alt=\"\"\n        />\n      </div>\n      <div className={styles['user-name']} onClick={handleShowLogin}>\n        {userLogged ? userInfo.profile.nickname : '未登录'}\n      </div>\n    </div>\n  )\n}\n\nexport default User\n","/Users/yejianfei/my_project/fate-music/src/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/fm/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/videos/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/download/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/my-cloud/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/setting/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/reducers/login/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/discovery/recommendation/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/videos/list/index.tsx",[],"/Users/yejianfe