rItemClick = async (\n    index: number,\n    targetId: number,\n    targetType: number\n  ) => {\n    setCurrentMid(index)\n    if (targetType === TARGET_TYPE.MUSIC) {\n      console.log(targetId)\n    }\n  }\n\n  return (\n    <div className={styles['banner-wrap']}>\n      <div className={styles.banners}>\n        {banners.map(({ pic, typeTitle, targetId, targetType }, index) => {\n          const className = bannersClassName[index] || styles.hidden\n\n          return (\n            <BannerItem\n              key={pic}\n              title={typeTitle}\n              pic={pic}\n              className={className}\n              onClick={() => handleBannerItemClick(index, targetId, targetType)}\n            />\n          )\n        })}\n      </div>\n      <div className={styles.dots}>\n        {banners.map((dot, index) => {\n          return (\n            <div\n              key={dot.pic}\n              className={cn(\n                styles.dot,\n                index === currentMid ? styles['dot-active'] : ''\n              )}\n              onMouseOver={() => handleBannerChange(index)}\n            />\n          )\n        })}\n      </div>\n    </div>\n  )\n}\n\nexport default Banner\n",["133","134"],"/Users/yejianfei/my_project/fate-music/src/components/link-title/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/layout/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/components/recommend-song-item/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/utils/index.ts",[],"/Users/yejianfei/my_project/fate-music/src/components/play-count/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/App.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/my-fm/index.tsx",[],"/Users/yejianfei/my_project/fate-music/src/views/discovery/song-list/index.tsx",["135"],"import React from 'react'\n\nimport * as recommendApis from 'apis/recommendation'\nimport useAsyncRequest from 'hooks/useAsyncRequest'\nimport RecommendSongItem fr